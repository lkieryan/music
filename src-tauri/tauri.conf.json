{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Music",
  "version": "0.1.0",
  "identifier": "in.kieran.music",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm web:dev",
    "beforeBuildCommand": "pnpm web:build"
  },
  "app": {
    "windows": [
      {
        "title": "Music",
        "width": 1500,
        "height": 1000,
        "resizable": true,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "android": {
      "minSdkVersion": 26
    }
  }
}
