{
    "name": "music",
    "version": "0.1.0",
    "private": true,
    "description": "A beautiful multi-source music player built with Rust and Tauri",
    "author": "kieran",
    "license": "GPL-2.0",
    "scripts": {
      "prepare": "cd frontend && npm install",
      "dev": "cd frontend && npm run dev",
      "build": "cd frontend && npm run build",
      "preview": "cd frontend && npm run preview",
      "tauri": "tauri",
      "dev:tauri": "tauri dev",
      "build:tauri": "tauri build",
      "lint": "cd frontend && npm run lint",
      "lint:fix": "cd frontend && npm run lint:fix",
      "clean": "rimraf frontend/dist src-tauri/target"
    },
    "devDependencies": {
      "@tauri-apps/cli": "^1.5.0",
      "rimraf": "^5.0.0",
      "cross-env": "^7.0.3"
    },
    "workspaces": [
      "frontend"
    ]
